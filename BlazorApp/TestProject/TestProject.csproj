        <PackageReference Include="NUnit3TestAdapter" Version="4.6.0" />
        <PackageReference Include="xunit" Version="2.9.2" />
    </ItemGroup>

    <ItemGroup>
        <Using Include="NUnit.Framework" />
    </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="..\BlazorApp\BlazorApp.csproj" />
    </ItemGroup>

